{
  "name": "slt",
  "version": "2.0.0",
  "description": "Take care of your state",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "nodemon -w __tests__ -w ./src -e js,jsx -x npm test"
  },
  "author": "Alexey Frolov <frol.rage@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "flux",
    "relay",
    "state",
    "immutable",
    "isomorphic",
    "promises"
  ],
  "devDependencies": {
    "babel-jest": "^5.2.0",
    "jest-cli": "^0.4.5",
    "express": "^4.13.1",
    "supertest": "^1.0.1",
    "core-js": "^0.9.18"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "immutable": "^3.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:AlexeyFrolov/slt.git"
  },
  "jest": {
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "/lib/*",
      "/src/*",
      "/node_modules/",
      "/data/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/data/",
      "/cms/",
      "/src/"
    ],
    "collectCoverage": false
  }
}
